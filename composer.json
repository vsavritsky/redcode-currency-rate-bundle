{
    "name": "savritsky/currency-rate-bundle",
    "type": "symfony-bundle",
    "description": "Symfony5+ currency rate bundle, allows convert currencies and load rates via cbr or ecb provider",
    "keywords": ["currency", "rate", "converter", "loader", "symfony", "bundle", "cbr.ru", "www.ecb.europa.eu"],
    "homepage": "https://github.com/maZahaca/redcode-currency-rate-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Reddikh",
            "email": "andrew@reddikh.com",
            "homepage": "http://reddikh.com"
    	}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "symfony/console": "^5.0|^6.0",
        "symfony/event-dispatcher": "^5.0|^6.0",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/twig-bundle": "^5.0|^6.0",
        "doctrine/orm": ">=2.3.2",
        "doctrine/dbal": ">=2.3.2",
        "redcode/currency-rate": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "autoload": {
        "psr-0": { "RedCode\\CurrencyRateBundle": "" }
    },
    "target-dir": "RedCode/CurrencyRateBundle"
}
